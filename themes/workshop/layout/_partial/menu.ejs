<div class="custom-wrapper" id="menu">
    <div class="menu-wrap  wrapper-full pure-g">
        <div class="pure-u-1 pure-u-md-2-5">
            <div class="pure-menu">
                <h1><a href="/" class="pure-menu-heading">
                 <img class="site-logo" src="<%- theme.assets %>itsacoyoteworkshop-logo.png" alt="Itsacoyote Workshop">
                </a></h1>
                <a href="#" class="custom-toggle" id="toggle"><s class="bar"></s><s class="bar"></s></a>
            </div>
        </div>
        <div class="pure-u-1 pure-u-md-3-5 main-menu">
            <div class="pure-menu pure-menu-horizontal">
                <ul class="pure-menu-list">
                    <% for (var i in theme.menu){ %>
                        <li class="pure-menu-item">
                            <a class="pure-menu-link" href="<%- url_for(theme.menu[i]) %>">
                                <%= i %>
                            </a>
                        </li>
                        <% } %>
                </ul>
            </div>
        </div>
    </div>
</div>